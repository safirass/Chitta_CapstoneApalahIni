{
  "expo": {
    "name": "CHITTA",
    "slug": "CHITTA",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/chitta.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/chitta.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-health-connect",
        {
          "permissions": [
            "read_sleep",
            "read_stress",
            "read_heart_rate",
            "read_steps"
          ]
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "minSdkVersion": 26
          }
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/chitta.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.CHITTA",
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "favicon": "./assets/chitta.png"
    },
    "extra": {
      "eas": {
        "projectId": "f82850bd-03e2-4f88-a9a0-daa76ec51029"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/f82850bd-03e2-4f88-a9a0-daa76ec51029"
    }
  }
}
